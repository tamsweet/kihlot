{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "7.2.5",
        "anandsiddharth/laravel-paytm-wallet": "^1.0",
        "anhskohbo/no-captcha": "^3.2",
        "apichef/laravel-pay-here": "^0.9.6",
        "ar-php/ar-php": "^1.0",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "bigbluebutton/bigbluebutton-api-php": "^2.0",
        "braintree/braintree_php": "4.3.*",
        "cartalyst/stripe-laravel": "12.0",
        "deviny/bbb-php-api": "^1.4",
        "devmarketer/easynav": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "instamojo/instamojo-php": "^0.4.0",
        "intervention/image": "^2.4",
        "jorenvanhocht/laravel-share": "^3.2",
        "kingflamez/laravelrave": "^2.0",
        "laravel/framework": "7.*",
        "laravel/helpers": "^1.2",
        "laravel/passport": "^9.3",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravolt/avatar": "^3.2",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mollie/laravel-mollie": "^2.9",
        "obydul/laraskrill": "^1.0",
        "orangehill/iseed": "^2.6",
        "paypal/rest-api-sdk-php": "^1.14",
        "razorpay/razorpay": "^2.5",
        "realrashid/sweet-alert": "^3.1",
        "revolution/socialite-amazon": "^1.1",
        "smodav/mpesa": "^5.1",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-image-optimizer": "^1.5",
        "spatie/laravel-query-builder": "^3.0",
        "spatie/laravel-sitemap": "^5.8",
        "spatie/laravel-translatable": "^4.3",
        "tzsk/payu": "^4.0",
        "unicodeveloper/laravel-paystack": "1.0.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "composer/composer": "^1.9",
        "filp/whoops": "^2.7",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "tanmuhittin/laravel-google-translate": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.2.5"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
